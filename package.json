{
  "name": "mappersmith",
  "version": "0.3.0",
  "description": "It is a lightweight rest client mapper for javascript",
  "author": "Tulio Ornelas <ornelas.tulio@gmail.com>",
  "contributors": [
    "Daniel Alves <alves.lopes.dan@gmail.com>",
    "Roberto Soares <roberto.tech@gmail.com>"
  ],
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/browserify index.js -s Mappersmith -o build/mappersmith.js -i ./src/gateway/node-*.js",
    "build-test": "./node_modules/.bin/browserify index.js -t rewireify -s Mappersmith -i ./src/gateway/node-*.js -o build/mappersmith.test.js",
    "release": "npm run build && ./node_modules/.bin/uglifyjs build/mappersmith.js -o build/mappersmith.min.js -c --screw-ie8 --preamble '/*! Mappersmith - https://github.com/tulios/mappersmith - Generated by browserify */'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tulios/mappersmith.git"
  },
  "keywords": [
    "rest-client",
    "isomorphic",
    "browser",
    "data-mapper"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tulios/mappersmith/issues"
  },
  "homepage": "https://github.com/tulios/mappersmith",
  "devDependencies": {
    "browserify": "^7.0.1",
    "chai": "^1.10.0",
    "mocha": "^2.0.1",
    "rewireify": "0.0.13",
    "shared-examples-for": "^0.1.3",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.6.0",
    "uglify-js": "^2.4.16"
  }
}
